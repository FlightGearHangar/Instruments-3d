   		</axis>
	</animation>

	<animation>
   		<type>textranslate</type>
   		<object-name>nm.102</object-name>
   		<property>instrumentation/dme/indicated-distance-nm</property>
   		<factor>0.1</factor>
   		<step>1</step>
   		<axis>
     		<x>1</x>
