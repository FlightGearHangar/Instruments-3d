<PropertyList>

<path>primus2000-eicas.ac</path>

<animation>
    <type>select</type>
	<object-name>gear0.annun</object-name>
  	<condition>
    	<equals>
    	<property>gear/gear[0]/position-norm</property>
    	<value>1.0</value>
    	</equals>
  	</condition>
</animation>

